<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.kotlin.order">

    <application
        android:name="com.kotlin.base.common.BaseApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".ui.activity.ShipAddressActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.OrderActivity" />
        <activity android:name=".ui.activity.ShipAddressEditActivity" />
        <activity android:name=".ui.activity.OrderConfirmActivity" />
        <activity android:name=".ui.activity.OrderDetailActivity" />

    </application>

</manifest>
